{
    "name": "bitboss/manageterminalusers",
    "description": "This is my package manageterminalusers",
    "keywords": [
        "BitBoss",
        "laravel",
        "manageterminalusers"
    ],
    "homepage": "https://github.com/bitboss/manageterminalusers",
    "license": "MIT",
    "authors": [
        {
            "name": "Riccardo Barbotti",
            "email": "riccardo@bitboss.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "BitBoss\\ManageTerminalUsers\\": "src",
            "BitBoss\\ManageTerminalUsers\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitBoss\\ManageTerminalUsers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitBoss\\ManageTerminalUsers\\ManageTerminalUsersServiceProvider"
            ],
            "aliases": {
                "ManageTerminalUsers": "BitBoss\\ManageTerminalUsers\\Facades\\ManageTerminalUsers"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
